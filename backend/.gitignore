$gitignore = @"
# Node / Next / TypeScript
.env
node_modules
.next
dist
build
.DS_Store
.env
.env.local
.vscode
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-lock.yaml
package-lock.json

# Optional per-folder ignores
/frontend/node_modules
/backend/node_modules
"@

$gitignore | Out-File -Encoding utf8 .gitignore
